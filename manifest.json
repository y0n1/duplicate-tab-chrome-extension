{
  "name": "Duplicate Tab for Chrome",
  "description": "Adds a keyboard shortcut which enables to duplicate the current tab in Google Chrome.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "duplicate-tab": {
      "suggested_key": {
        "default": "Alt+D",
        "mac": "Alt+D"
      },
      "description": "Duplicates the current tab"
    }
  }
}
